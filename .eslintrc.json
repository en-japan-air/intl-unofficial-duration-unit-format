{
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "es2021": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "comma-dangle": ["error", {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "functions": "always-multiline"
    }]
  }
}
